{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "origin-when-cross-origin"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/dunkertons/:path*",
            "destination": "https://dunkertons-sales-dashboard.vercel.app/dunkertons/:path*"
        }
    ]
}